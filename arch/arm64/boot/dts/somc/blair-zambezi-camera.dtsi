&pm6125_gpios {
	camera_flash_pwm {
		camera_flash_pwm_default: camera_flash_pwm_default {
			pins = "gpio8";
			function = "func1";
			input-disable; /* No PULL */
			output-low;
			bias-disable;
			power-source = <0>;
			drive-strength = <3>;
		};
	};
};
