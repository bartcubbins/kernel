#include <dt-bindings/clock/qcom,gcc-blair.h>
#include <dt-bindings/msm/msm-camera.h>

&soc {
	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		gpios-shared = <366>;
		pinctrl-names = "flash_active", "flash_suspend";
		pinctrl-0 = <&flash_active>;
		pinctrl-1 = <&flash_suspend>;
		status = "okay";
	};

	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		/* TODO */
		status = "okay";
	};

	cam_vdig: cam_vdig {
		compatible = "regulator-fixed";
		regulator-name = "cam_vdig";
		startup-delay-us = <0>;
		enable-active-high;
		status = "okay";
	};

	cam_vana: cam_vana {
		compatible = "regulator-fixed";
		regulator-name = "cam_vana";
		startup-delay-us = <0>;
		enable-active-high;
		status = "okay";
	};

	cam_vaf1: cam_vaf1 {
		compatible = "regulator-fixed";
		regulator-name = "cam_vaf1";
		startup-delay-us = <0>;
		enable-active-high;
		status = "okay";
	};

	cam_vaf2: cam_vaf2 {
		compatible = "regulator-fixed";
		regulator-name = "cam_vaf2";
		startup-delay-us = <0>;
		enable-active-high;
		status = "okay";
	};

	cam_vio: cam_vio {
		compatible = "regulator-fixed";
		regulator-name = "cam_vio";
		startup-delay-us = <0>;
		enable-active-high;
		status = "okay";
	};
};

&cam_cci0 {
	#address-cells = <1>;
	#size-cells = <0>;

	// Main camera
	actuator0: qcom,actuator@0 {
		cell-index = <0>;
		compatible = "qcom,actuator";
		cci-master = <0>;
		/* TODO */
		status = "okay";
	};

	eeprom0: qcom,eeprom@0 {
		cell-index = <0>;
		compatible = "qcom,eeprom";
		cci-master = <0>;
		/* TODO */
		status = "okay";
	};

	ois0: qcom,ois@0 {
		cell-index = <0>;
		compatible = "qcom,ois";
		cci-master = <0>;
		/* TODO */
		status = "okay";
	};

	sensor0: qcom,cam-sensor@0 {
		cell-index = <0>;
		compatible = "qcom,cam-sensor";
		cci-master = <0>;
		csiphy-sd-index = <0>;
		sensor-position-roll = <90>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <180>;
		actuator-src = <&actuator0>;
		eeprom-src = <&eeprom0>;
		led-flash-src = <&led_flash_rear>;
		ois-src = <&ois0>;
		/* TODO */
		//cam_vio-supply = <>;
		//cam_vana-supply = <>;
		//cam_vaf-supply = <>;
		//cam_vdig-supply = <>;
		cam_clk-supply = <&gcc_camss_top_gdsc>;
		regulator-names = "cam_vio", "cam_vana", "cam_vaf", "cam_vdig", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 2900000 2800000 1100000 0>;
		rgltr-max-voltage = <1800000 2900000 2800000 1100000 0>;
		rgltr-load-current = <0 0 0 0 0>;
		pwm-switch;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active &cam_sensor_rear0_reset_active &cam_ois_vdd_active>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend &cam_sensor_rear0_reset_suspend &cam_ois_vdd_suspend>;
		gpios = <&tlmm 29 0>, <&tlmm 34 0>, <&tlmm 33 0>;
		gpio-reset = <1>;
		gpio-standby = <2>;
		gpio-req-tbl-num = <0 1 2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK0", "CAM_RESET0";
		clocks = <&gcc GCC_CAMSS_MCLK0_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <19200000>;
		status = "okay";
	};

	// Wide camera
	eeprom1: qcom,eeprom@1 {
		cell-index = <1>;
		compatible = "qcom,eeprom";
		cci-master = <1>;
		/* TODO */
		status = "okay";
	};

	sensor1: qcom,cam-sensor@1 {
		cell-index = <1>;
		compatible = "qcom,cam-sensor";
		cci-master = <1>;
		csiphy-sd-index = <1>;
		sensor-position-roll = <90>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <180>;
		eeprom-src = <&eeprom1>;
		led-flash-src = <&led_flash_rear>;
		/* TODO */
		//cam_vio-supply = <>;
		//cam_vana-supply = <>;
		//cam_vdig-supply = <>;
		cam_clk-supply = <&gcc_camss_top_gdsc>;
		regulator-names = "cam_vio", "cam_vana", "cam_vdig", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 2800000 1200000 0>;
		rgltr-max-voltage = <1800000 2800000 1200000 0>;
		rgltr-load-current = <0 0 0 0>;
		pwm-switch;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_active &cam_sensor_rear1_reset_active>;
		pinctrl-1 = <&cam_sensor_mclk1_suspend &cam_sensor_rear1_reset_suspend>;
		gpios = <&tlmm 30 0>, <&tlmm 35 0>;
		gpio-reset = <1>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1", "CAM_RESET1";
		clocks = <&gcc GCC_CAMSS_MCLK1_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <19200000>;
		status = "okay";
	};
};

