&qupv3_se10_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	wl2868c@2f {
		compatible = "willsemi,wl2868c";
		reg = <0x2f>;
		rstn-gpio = <&tlmm 58 0>;
		vin12-supply = <&cam_vdig>;
		vin34-supply = <&cam_vana>;
		vin5-supply  = <&cam_vaf1>;
		vin6-supply  = <&cam_vaf2>;
		vin7-supply  = <&cam_vio>;

		regulators {
			wl2868c_ldo1: ldo1 {
				regulator-name = "cam-dvdd1";
				regulator-min-microvolt = <496000>;
				regulator-max-microvolt = <1512000>;
			};

			wl2868c_ldo2: ldo2 {
				regulator-name = "cam-dvdd2";
				regulator-min-microvolt = <496000>;
				regulator-max-microvolt = <1512000>;
			};

			wl2868c_ldo3: ldo3 {
				regulator-name = "cam-avdd1";
				regulator-min-microvolt = <1504000>;
				regulator-max-microvolt = <3544000>;
			};

			wl2868c_ldo4: ldo4 {
				regulator-name = "cam-avdd2";
				regulator-min-microvolt = <1504000>;
				regulator-max-microvolt = <3544000>;
			};

			wl2868c_ldo5: ldo5 {
				regulator-name = "cam-vddaf";
				regulator-min-microvolt = <1504000>;
				regulator-max-microvolt = <3544000>;
			};

			wl2868c_ldo6: ldo6 {
				regulator-name = "cam-vddois";
				regulator-min-microvolt = <1504000>;
				regulator-max-microvolt = <3544000>;
			};

			wl2868c_ldo7: ldo7 {
				regulator-name = "cam-vddio";
				regulator-min-microvolt = <1504000>;
				regulator-max-microvolt = <3544000>;
			};
		};
	};
};

&tlmm {
	cci3_active: cci3_active {
		mux {
			/* CLK, DATA */
			pins = "gpio2", "gpio3";
			function = "CCI_I2C";
		};

		config {
			pins = "gpio2", "gpio3";
			bias-pull-up; /* PULL UP*/
			drive-strength = <2>; /* 2 MA */
		};
	};

	cci3_suspend: cci3_suspend {
		mux {
			/* CLK, DATA */
			pins = "gpio2", "gpio3";
			function = "CCI_I2C";
		};

		config {
			pins = "gpio2", "gpio3";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	flash_active: flash_active {
		mux {
			pins = "gpio11";
			function = "gpio";
		};

		config {
			pins = "gpio11";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	flash_suspend: flash_suspend {
		mux {
			pins = "gpio11";
			function = "gpio";
		};

		config {
			pins = "gpio11";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	cam_ois_vdd_active: cam_ois_vdd_active {
		mux {
			pins = "gpio33";
			function = "gpio";
		};

		config {
			pins = "gpio33";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_ois_vdd_suspend: cam_ois_vdd_suspend {
		mux {
			pins = "gpio33";
			function = "gpio";
		};

		config {
			pins = "gpio33";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

